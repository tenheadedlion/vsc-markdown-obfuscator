{
  "name": "Markdown-Obfuscate",
  "displayName": "Markdown Obfuscate",
  "description": "Markdown Obfuscate",
  "repository": "https://github.com/tenheadedlion/vsc-markdown-obfuscator.git",
  "version": "0.0.1",
  "publisher": "TemporaryValues",
  "license": "LICENSE",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.obfuscateImages"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.obfuscateImages",
        "title": "Obfuscate Markdown Images"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == markdown",
          "command": "extension.obfuscateImages",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "exif-be-gone-ts": "^0.0.8",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
